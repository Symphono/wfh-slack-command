{
	"version": "0.2.0",
	"configurations": [
		{
			"env": {
				"GOOGLE_TARGET_CALENDAR": "your_wfh_calendar@resource.calendar.google.com",
				"GOOGLE_PRIVATE_KEY": "-----BEGIN PRIVATE KEY-----\nyourkey\n-----END PRIVATE KEY-----\n",
